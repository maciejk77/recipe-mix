    "autoprefixer": "^10.0.2",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.23.0",
    "cloudinary-react": "^1.6.7",
    "deepmerge": "^4.2.2",
    "graphql": "^15.4.0",
    "js-cookie": "^2.2.1",
    "micro-cors": "^0.1.1",
    "next": "10.0.1",
    "next-auth": "^4.22.1",
    "postcss": "^8.1.7",
